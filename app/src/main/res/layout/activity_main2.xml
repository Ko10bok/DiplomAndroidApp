<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#121111"
    android:backgroundTint="#3F3F3F"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextText9"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textColor="#FCF5FD"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_column="1"
        app:layout_constraintTop_toBottomOf="@+id/editTextText8"
        app:layout_row="1"
        tools:layout_editor_absoluteX="224dp" />

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textColor="#FCF5FD"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_column="1"
        app:layout_constraintTop_toBottomOf="@+id/editTextText7"
        app:layout_row="1"
        tools:layout_editor_absoluteX="224dp" />

    <EditText
        android:id="@+id/editTextText6"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginTop="91dp"
        android:ems="10"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textColor="#FCF5FD"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_column="1"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_row="1"
        tools:layout_editor_absoluteX="224dp" />

    <EditText
        android:id="@+id/editTextText7"
        android:layout_width="89dp"
        android:layout_height="40dp"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textColor="#FCF5FD"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_column="1"
        app:layout_constraintTop_toBottomOf="@+id/editTextText6"
        app:layout_row="1"
        tools:layout_editor_absoluteX="224dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="16dp"
        android:text="Тип двигателя"
        android:textAlignment="center"
        android:textColor="#FCF5FD"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="16dp"
        android:text="Год выпуска"
        android:textAlignment="center"
        android:textColor="#FCF5FD"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <Button
        android:id="@+id/button5"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#3F3F3F"
        android:textAlignment="center"
        app:icon="@android:drawable/ic_dialog_info"
        app:iconGravity="top"
        app:iconPadding="14dp"
        app:iconTintMode="multiply"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="400dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:autoText="false"
        android:backgroundTint="#FF9800"
        android:text="Ввод характеристик"
        android:textColor="#070607"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="198dp"
        android:layout_height="60dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#FF9800"
        android:text="Подобрать"
        android:textColor="#070607"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button4" />

    <Button
        android:id="@+id/button4"
        android:layout_width="198dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#FF9800"
        android:text="База данных"
        android:textColor="#070607"
        android:textSize="16sp"
        app:cornerRadius="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="295dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        android:text="Подбор масла(Демо)"
        android:textAlignment="center"
        android:textColor="#FCF5FD"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="91dp"
        android:layout_marginEnd="16dp"
        android:text="Марка автомобиля"
        android:textAlignment="center"
        android:textColor="#FCF5FD"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="158dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="16dp"
        android:text="Модель"
        android:textAlignment="center"
        android:textColor="#FCF5FD"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

</androidx.constraintlayout.widget.ConstraintLayout>